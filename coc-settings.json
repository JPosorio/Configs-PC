{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "javascript",
    "javascriptreact"
  ],
  "tailwindCSS.headwind.runOnSave": true,
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextAlign": "below"
}

